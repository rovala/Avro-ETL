{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "java (build)",
			"paths": [
				"${workspace}"
			],
			"isFullBuild": true,
			"group": "build",
			"problemMatcher": [],
			"label": "java (build): Build Workspace",
			"detail": "$(tools) Build all the Java projects in workspace.",
			"windows": {
				"commad": "C:\\Program Files\\Java\\jdk-11.0.16.1\\bin\\javac.exe"
			}
		},
		{
			"type": "java (buildArtifact)",
			"mainClass": "etl.ini.Empezar",
			"targetPath": "${workspaceFolder}/${workspaceFolderBasename}.jar",
			"elements": [
				"${compileOutput}",
				"${dependencies}"
			],
			"problemMatcher": [],
			"label": "java (buildArtifact): Avro",
			"group": {
				"kind": "build",
				"isDefault": true
			}
		}
	]
}